<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Yahoo WebScraper</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @{
        Html.RenderPartial("_HeaderNavBar");
    }
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <h5>Financial Scraper</h5>
            <i style="padding: 6px">Typed by the fingers of:</i>
            <p>&copy; @DateTime.Now.Year - Jim Doyle, Student</p>
            <a href="http://careerdevs.com">CareerDevs University</a>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>